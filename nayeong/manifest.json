{
    "manifest_version": 2,
  
    "name": "WeatherInfo",
    "description": "This extension allows the user to check the weather simply.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },

    "permissions": [
      "activeTab",
      "geolocation"
    ]
  }